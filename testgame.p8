--updates
function update_game()
 if talkwind then
  if getbtn()==5 then
   talkwind.dur=0
   talkwind=nil
  end
 else
  dobtnbuff()
 	dobtn(btnbuff)
  btnbuff=-1
 end
end

function update_pturn()
 dobtnbuff()
 p_t=min(p_t+0.2,1)
 p_mob:mov(p_mob)

 if p_t==1 then
 	_upd=update_game
 	if checkend() then
   doai()
 	end
 end
 calcdist(p_mob.x,p_mob.y)
end

function update_aiturn()
 dobtnbuff()
 p_t=min(p_t+0.2,1)
 for m in all(mob) do
  if m!=p_mob and m.mov then
   	m:mov(m)
  end
 end
 if p_t==1 then
 	_upd=update_game
  checkend()
 end
end

function update_gover()
 if btnp(❎) then
  fadeout()
  startgame()
 end
end

function dobtnbuff()
 if btnbuff==-1 then
	 btnbuff=getbtn()
	end
end

function getbtn()
 for i=0,5 do
	 if btnp(i) then
	 	return i
 	end
 end
 return -1
end

function dobtn(btn)
 if btn<0 then return end
 if btn<4 then
	 moveplayer(dirx[btn+1],diry[btn+1])
 end
end

