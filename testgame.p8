--updates
function update_game()
 if buttbuff==-1 then
	 buttbuff=getbutt()
	end

	dobutt(buttbuff)
 buttbuff=-1
end

function update_pturn()
 if buttbuff==-1 then
	 buttbuff=getbutt()
	end
	
 p_t=min(p_t+0.2,1)
 
 p_mov()

 if p_t==1 then
 	_upd=update_game
 end
end

function mov_walk()
 p_ox=p_sox*(1-p_t)
 p_oy=p_soy*(1-p_t)
end

function mov_bump()
 local tme=p_t
 if p_t>0.5 then
 	tme=1-p_t
 end
  p_ox=p_sox*(tme)
  p_oy=p_soy*(tme)
end

function update_gameover()
end

function getbutt() 
 for i=0,5 do 
	 if btnp(i) then
	 	return i
 	end
 end
 return -1
end

function dobutt(butt)
 if butt<0 then return end
 if butt>=0 and butt<4 then
	 moveplayer(dirx[butt+1],diry[butt+1])
  return
 end
